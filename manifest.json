{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "小窗口视频 Picture-in-Picture Extension",
  "description": "tampermonkey脚本，小窗口视频的插件版；Watch video using Picture-in-Picture",
  "version": "0.1",
  "icons": {
    "128": "assets/icon128.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": {
      "19": "assets/icon19.png",
      "38": "assets/icon38.png"
    }
  },
  "commands": {
    "left": {
      "description": "left",
      "suggested_key": {
        "windows": "Ctrl+Left",
        "mac": "Command+Left",
        "chromeos": "Ctrl+Left",
        "linux": "Ctrl+Left"
      }
    },
    "right": {
      "description": "right",
      "suggested_key": {
        "windows": "Ctrl+Right",
        "mac": "Command+Right",
        "chromeos": "Ctrl+Right",
        "linux": "Ctrl+Right"
      }
    },
    "up": {
      "description": "up",
      "suggested_key": {
        "windows": "Ctrl+Up",
        "mac": "Command+Up",
        "chromeos": "Ctrl+Up",
        "linux": "Ctrl+Up"
      }
    },
    "down": {
      "description": "down",
      "suggested_key": {
        "windows": "Ctrl+Down",
        "mac": "Command+Down",
        "chromeos": "Ctrl+Down",
        "linux": "Ctrl+Down"
      }
    }
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "permissions": [
    "<all_urls>",
    "storage"
  ],
  "minimum_chrome_version": "69.0.3483.0",
  "manifest_version": 2
}
